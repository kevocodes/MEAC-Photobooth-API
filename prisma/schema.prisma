generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Photography {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  code      String   @unique
  url       String
  width     Int
  height    Int
  public_id String
  
  printedAt DateTime?
  printedQuantity Int?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("photographies")
}
